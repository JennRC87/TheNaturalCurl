<%= form_for :rock, method: 'patch', url: {action: 'update'} do |p| %>
<%= label(:post, :title) %><br>
<%= p.text_field :title %><br>
<%= label(:post, :image) %><br>
<%= p.text_field :image %><br>
<%= label(:post, :content) %><br>
<%= p.text_field :content %><br>
<%= label(:post, :post_category) %><br>
<%= p.select(:post_category, options_for_select([['Washing', 'washing'], ['Add Moisture', 'add_moisture'], ['Drying', 'drying'], ['Cutting', 'cutting'],['Styling', 'styling'],['Products', 'products']])) %><br>
<%= p.submit "Save Posts" %>
<% end %>

